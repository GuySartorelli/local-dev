{
  "name": "guysartorelli/dev-tools",
  "description": "Dev tools CLI for my idiosyncratic development setup",
  "license": "BSD-3-Clause",
  "bin": [
    "bin/dev-tools"
  ],
  "autoload": {
    "psr-4": {
      "DevTools\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "symfony/console": "^6.0",
    "symfony/process": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/filesystem": "^6.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
